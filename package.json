{
  "name": "kevoree-comp-mqttsubclient",
  "version": "2.0.4-beta",
  "main": "./lib/MQTTSubClient.js",
  "description": "Subscribes to the specified MQTT <strong>host</strong>:<strong>port</strong> and <strong>topic</strong> and broadcasts the published messages to its output ports",
  "kevoree": {
    "namespace": "kevoree"
  },
  "scripts": {
    "prepublish": "npm run lint && grunt",
    "postpublish": "grunt publish",
    "lint": "eslint lib"
  },
  "keywords": [
    "kevoree-std-lib",
    "kevoree",
    "library",
    "comp"
  ],
  "dependencies": {
    "kevoree-entities": "^9.0.0",
    "mqtt": "^2.1.3"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "grunt": "^1.0.1",
    "grunt-kevoree": "^5.9.1",
    "grunt-kevoree-genmodel": "^3.0.1",
    "grunt-kevoree-registry": "^3.0.3",
    "load-grunt-tasks": "^3.5.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ]
  }
}
